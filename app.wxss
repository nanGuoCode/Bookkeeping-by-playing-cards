/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page,view,text,swiper,swiper-item,image,navigator
{
padding: 0;
margin: 0;
box-sizing: border-box;
}

.clBlack{
  color: #222222;
}

.clOrange{
  color: #e6733a;
}

.clGray1{
  color: #666666;
}

.clGray2{
  color: #999999;
}

.clBkOrange{
  background-color: #e6733a;
}

.bottom{
  margin-top: 150rpx;
  display: flex;
  justify-content: center;
}

.bottomLeft,.bottomRight{
  color: #030303;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  border: 2rpx solid #999999;
  text-align: center;
  border-radius: 40rpx;
}

.bottomLeft{
  width: 120rpx;
}
.bottomRight{
  width: 180rpx;
}


